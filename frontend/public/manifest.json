{
  "name": "Travel Planner - AI-Powered Trip Planning",
  "short_name": "Travel Planner",
  "description": "Plan your perfect trip with AI-powered itineraries, smart packing lists, and reservation management",
  "version": "1.0.0",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#2563eb",
  "orientation": "portrait-primary",
  "scope": "/",
  "id": "travel-planner-pwa",
  "prefer_related_applications": false,
  "icons": [
    {
      "src": "/favicon.ico",
      "sizes": "64x64 32x32 24x24 16x16",
      "type": "image/x-icon"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "192x192",
      "type": "image/svg+xml",
      "purpose": "maskable any"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "purpose": "maskable any"
    }
  ],
  "shortcuts": [
    {
      "name": "Create New Trip",
      "short_name": "New Trip",
      "description": "Start planning a new travel itinerary",
      "url": "/create",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ]
    },
    {
      "name": "My Trips",
      "short_name": "My Trips",
      "description": "View all your saved itineraries",
      "url": "/trips",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ]
    },
    {
      "name": "Weather Check",
      "short_name": "Weather",
      "description": "Check weather for your destinations",
      "url": "/weather-test",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ]
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/desktop.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Home screen showing travel itineraries"
    },
    {
      "src": "/screenshots/mobile.png",
      "sizes": "375x667",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Mobile itinerary view with activities"
    }
  ],
  "categories": ["travel", "productivity", "lifestyle"],
  "lang": "en",
  "dir": "ltr",
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["text/calendar", "application/json", "image/*"]
        }
      ]
    }
  },
  "file_handlers": [
    {
      "action": "/import",
      "accept": {
        "text/calendar": [".ics"],
        "application/json": [".json"]
      }
    }
  ]
}
